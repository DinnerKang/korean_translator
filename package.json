{
    "name": "korean-translator",
    "displayName": "Korean-translator",
    "description": "한->영 번역기",
    "version": "0.3.0",
    "publisher": "dinner",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "korean",
        "번역기",
        "translator",
        "translate",
        "translation"
    ],
    "bugs": {
        "url": "https://github.com/DinnerKang/korean_translator/issues",
        "email": "jenhyuk0318@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DinnerKang/korean_translator.git"
    },
    "icon": "asset/logo.png",
    "activationEvents": [
        "onCommand:extension.translateKorean"
    ],
    "main": "./out/extension",
    "contributes": {
        "menus": {
            "editor/context": [
                {
                    "command": "extension.translateKorean",
                    "when": "editorHasSelection"
                }
            ]
        },
        "commands": [
            {
                "command": "extension.translateKorean",
                "title": "Translate !!!",
                "when": "editorHasSelection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.26"
    },
    "dependencies": {
        "event-stream": "^4.0.1",
        "firebase-admin": "^7.0.0",
        "isomorphic-fetch": "^2.2.1",
        "npm": "^6.4.1",
        "vsce": "^1.53.2"
    }
}
